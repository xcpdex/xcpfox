        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "date-fns": "^1.29.0",
        "font-awesome": "^4.7.0",
        "highcharts": "^6.1.0",
        "laravel-vue-pagination": "^1.2.0",
        "vue-timeago": "^4.0.1",
        "vue-truncate-filter": "^1.1.7",
        "vue2-highcharts": "^1.2.3",
        "vuejs-auto-complete": "^0.6.5",
        "vuejs-paginator": "^2.0.2"
